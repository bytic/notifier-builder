{
  "name": "bytic/notifier-builder",
  "description": "notifier builder module for bytic library",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/notifier-builder",
  "require": {
    "bytic/console": "^1.0",
    "bytic/models-smart-properties": "^1.0",
    "bytic/notifier": "^1.0",
    "bytic/orm": "^0.9 || ^1.0",
    "bytic/orm-behaviors": "^0.9 || ^1.0",
    "bytic/orm-filters": "^0.9 || ^1.0",
    "bytic/package-base": "^1.0",
    "bytic/mail-templates": "^1.0",
    "bytic/translation": "^0.9 || ^1.0"
  },
  "require-dev": {
    "bytic/application": "^1.0",
    "bytic/dispatcher": "^1.0",
    "bytic/migrations": "^0.12",
    "bytic/phpqatools": "^1.0",
    "bytic/staging": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ByTIC\\NotifierBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\NotifierBuilder\\Tests\\": "tests/src",
      "ByTIC\\NotifierBuilder\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "bytic/phpqatools": true
    }
  }
}
